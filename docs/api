get admins/products "get all products"
post admins/products "create product"
get admins/products/{id} "get product by id"
put admins/products/{id} "update product"
delete admins/products/{id} "delete product"

get admins/reviews "get all reviews"
post admins/reviews "create review" ?
get admins/reviews/{id} "get product by id"
delete admins/reviews/{id} "delete product"

get admins/users "get all users"
post admins/users "create user"
get admins/users/{id} "get user by id" 
put admins/users/{id} "update user"
delete admins/users/{id} "delete user"

get admins/categories "get all categories"
post admins/categories "create category"
get admins/categories/{id} "get category by id"
put admins/categories/{id} "update category"
delete admins/categories/{id} "delete category"

get users/account "get current user account info"
post users/sign-in "sign in"
post users/sign-up "sign up"

post products/{id}/reviews "create review for product"
put/patch products/{id} "update product rating"